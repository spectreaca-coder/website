# Admin credentials for development only
# Copy this file to .env and update with your credentials
# IMPORTANT: In production, use a proper backend authentication system
REACT_APP_ADMIN_USERNAME=specter123
REACT_APP_ADMIN_PASSWORD=admin1031!

# Firebase 설정
# Firebase Console에서 프로젝트를 생성하고 웹 앱 구성 정보를 입력하세요
# 설정 방법은 FIREBASE_SETUP.md 참고
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Google Sheets 연동 URL
# Google Apps Script 웹 앱 배포 후 받은 URL을 여기에 입력하세요
# 설정 방법은 google-apps-script/README.md 참고
REACT_APP_GOOGLE_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_URL_HERE/exec
